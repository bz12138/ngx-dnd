{
  "name": "ngx-dnd",
  "version": "7.0.0",
  "description": "Drag and Drop for Angular2 and beyond!",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:release": "cross-env NODE_ENV=production ng build --prod --no-aot --no-build-optimizer",
    "serve": "ng serve",
    "start": "npm-run-all serve",
    "start:prod": "npm-run-all build:lib:prod serve",
    "test": "start-server-and-test serve http-get://localhost:4200 cy:open",
    "cy:run": "cypress run --browser chrome",
    "cy:open": "cypress open",
    "test:single": "start-server-and-test serve http-get://localhost:4200 cy:run",
    "lint": "ng lint",
    "prettier": "prettier --write \"**/*.{js,ts}\"",
    "changelog": "chg release -y && git add -A CHANGELOG.md",
    "setMetadata": "ts-node ./tools/set-metadata.ts",
    "preversion": "npm-run-all build:lib:prod test:single",
    "version": "npm run changelog",
    "postversion": "npm run setMetadata",
    "scss-bundle": "scss-bundle -c ./projects/swimlane/ngx-dnd/scss-bundle.config.json",
    "scss-build": "node-sass ./projects/swimlane/ngx-dnd/src/lib/ngx-dnd.scss ./dist/swimlane/ngx-dnd/release/index.css",
    "copyFiles": "cpx \"./{CHANGELOG.md,README.md,LICENSE}\" ./dist/swimlane/ngx-dnd",
    "prepublish:lib": "npm-run-all build:lib:prod test:single",
    "publish:lib": "cd dist/swimlane/ngx-dnd && npm publish",
    "build:lib": "ng build @swimlane/ngx-dnd",
    "postbuild:lib": "npm-run-all copyFiles scss-bundle scss-build",
    "build:lib:prod": "ng build --prod @swimlane/ngx-dnd",
    "postbuild:lib:prod": "npm-run-all copyFiles scss-bundle scss-build",
    "pack:lib": "cd dist/swimlane/ngx-dnd && npm pack",
    "predeploy": "npm run build:release",
    "deploy": "angular-cli-ghpages --dir ./dist/ngx-dnd/",
    "publish": "npm-run-all publish:lib deploy",
    "prepublishOnly": "echo \"This is the workspace -- you probably meant run 'npm run lib:prepublish'.\" && exit 1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.13",
    "@angular/common": "^7.2.13",
    "@angular/compiler": "^7.2.13",
    "@angular/core": "^7.2.13",
    "@angular/elements": "^7.2.13",
    "@angular/forms": "^7.2.13",
    "@angular/http": "^7.2.13",
    "@angular/platform-browser": "^7.2.13",
    "@angular/platform-browser-dynamic": "^7.2.13",
    "@angular/router": "^7.2.13",
    "@ngx-loading-bar/core": "^2.2.0",
    "@ngx-loading-bar/http-client": "^2.2.0",
    "@stackblitz/sdk": "^1.2.0",
    "@swimlane/docspa-core": "^2.0.0",
    "@swimlane/docspa-remark-preset": "^2.0.0",
    "@swimlane/dragula": "^3.8.0",
    "@types/dragula": "^2.1.33",
    "@webcomponents/custom-elements": "^1.2.0",
    "core-js": "^2.5.4",
    "deepmerge": "^2.1.1",
    "ngx-logger": "^3.0.5",
    "path": "^0.12.7",
    "process": "^0.11.10",
    "quick-lru": "^1.1.0",
    "remark-inline-links": "^3.1.1",
    "rxjs": "^6.4.0",
    "smoothscroll-polyfill": "^0.4.3",
    "url-resolve": "^1.0.0",
    "vfile-reporter": "^5.0.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular-devkit/build-ng-packagr": "^0.13.8",
    "@angular/cli": "^7.3.8",
    "@angular/compiler-cli": "^7.2.13",
    "@angular/language-service": "^7.2.13",
    "@swimlane/prettier-config-swimlane": "^0.1.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/mdast": "^3.0.2",
    "@types/node": "^8.9.5",
    "angular-cli-ghpages": "^0.5.2",
    "chg": "^0.3.3",
    "codelyzer": "~4.2.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "cypress": "^3.2.0",
    "ng-packagr": "^5.1.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.12.1",
    "scss-bundle": "^2.5.1",
    "start-server-and-test": "^1.4.1",
    "ts-node": "~5.0.1",
    "tsickle": "^0.34.3",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "tslint-config-prettier": "^1.13.0",
    "tslint-config-swimlane": "^3.0.4",
    "typescript": "~3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swimlane/ngx-dnd.git"
  },
  "keywords": [
    "angular2",
    "angular4",
    "angular5",
    "dnd",
    "drag",
    "drop",
    "angular"
  ],
  "authors": [
    "Jayson Harshbarger"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swimlane/ngx-dnd/issues"
  },
  "homepage": "https://github.com/swimlane/ngx-dnd#readme"
}
